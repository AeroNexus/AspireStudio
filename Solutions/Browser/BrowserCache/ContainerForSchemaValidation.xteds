  <Interface name="Plan" id="10" description="Standard interface for a Plan service">

    <Variable name="PlanId" kind="ID" format="UINT32"/>
    <Variable name="PlanTime" kind="Time" format="FLOAT64" units="s"/>
    <Variable name="PlanFormat" kind="String" format="STRING"/>
    <!-- This could become a enumerated variable (perhaps with an OTHER option) but is a STRING for now to avoid the need for schema changes -->
    <Variable name="Plan" kind="String" format="STRING"/>
