  <!--=======================================-->
  <!-- ACTIVITY EXECUTION STANDARD INTERFACE -->
  <!--=======================================-->
  
  <Interface name="ActivityExecution" id="11" description="Standard interface for an activity execution service">

    <Variable name="ActivityId" kind="ID" format="UINT32" description="Unique activity identity"/>
    <Variable name="ActivityName" kind="String" format="INT08" length="33"/><!-- 33-byte, null terminated string -->
    <Variable name="ActivityPriority" kind="tbd" format="UINT8" rangeMin="0" rangeMax="255"/>
    <!-- 0 is the lowest priority and 255 is the highest -->
    <Variable name="StartTime" kind="Time" format="FLOAT64" units="s"/>
    <!-- 0.0 signifies now - i.e. as soon as possible -->
    <Variable name="ActivityRequestStatus" kind="Status" format="INT16">
      <Drange name="ActivityRequestStatusEnum">
        <Option value="0" name="INVALID_REQUEST"/>
        <!-- The request parameters were invalid -->
        <Option value="1" name="REQUEST_DENIED"/>
        <!-- The request was valid by the activity could not be executed as requested - e.g. there was a conflict -->
        <Option value="2" name="REQUEST_ACCEPTED"/>
        <!-- The activity has been accepted and its execution is pending -->
      </Drange>
    </Variable>

    <!-- Additional Variables that apply to all activities for a specific executive are defined here -->
    <!-- Additional Variables for activity unique parameters are defined here  - e.g. Lat, Lon, Elevation for an imaging activity -->

    <Request>
      <!-- If VariableRefs are added to this message, each extended message must be given a unique name - e.g. ExecuteImagingActivity -->
      <CommandMsg name="ExecuteActivity" id="1" description="Execute a mission or housekeeping activity at the specified time">
        <VariableRef name="ActivityId"/>
        <!-- Can be assigned by the requestor - e.g. the ground, but must be unique. If 0 it will be assigned by the ActivityPlanning service -->
        <VariableRef name="ActivityName"/>
        <VariableRef name="ActivityPriority"/>
        <VariableRef name="StartTime"/>
        <!-- Additional VariableRefs that apply to all activities for a specific executive are defined here -->
        <!-- Additional VariableRefs for activity unique parameters are defined here -->
      </CommandMsg>
      <DataReplyMsg name="ExecuteActivityReply" id="2">
        <VariableRef name="ActivityId"/>
        <VariableRef name="ActivityRequestStatus"/>
      </DataReplyMsg>
    </Request>

    <Command>
      <CommandMsg name="AbortActivity" id="3" description="Terminate (if necessary) and delete an executing or pending activity">
        <VariableRef name="ActivityId"/>
      </CommandMsg>
    </Command>

    <Command>
      <CommandMsg name="DeleteActivity" id="4" description="Delete a pending activity">
        <VariableRef name="ActivityId"/>
      </CommandMsg>
    </Command>

    <Command>
      <CommandMsg name="DeleteAllActivities" id="5" description="Delete all pending activities"/>
    </Command>

    <Request>
      <!-- If VariableRefs are added to this message, each extended message must be given a unique name - e.g. GetImagingActivityProperties -->
      <CommandMsg name="GetActivityProperties" id="6" description="Get the Activity's standard and unique properties">
        <VariableRef name="ActivityId"/>
      </CommandMsg>
      <DataReplyMsg name="GetActivityPropertiesReply" id="7">
        <VariableRef name="ActivityId"/>
        <VariableRef name="ActivityName"/>
        <VariableRef name="ActivityPriority"/>
        <VariableRef name="StartTime"/>
        <!-- Additional VariableRefs that apply to all activities for a specific planner are defined here -->
        <!-- Additional VariableRefs for activity unique parameters are defined here -->
      </DataReplyMsg>
    </Request>

    <Command>
      <!-- Typically used by a ground system to get an updated list of activities -->
      <CommandMsg name="SendActivityProperties" id="8" description="Send an ActivityPropertiesResponse for each executing and pending activity"/>
    </Command>

    <Notification>
      <!-- If VariableRefs are added to this message, each extended message must be given a unique name - e.g. ImagingActivityPropertiesResponse -->
      <DataMsg name="ActivityPropertiesResponse" id="9" msgArrival="EVENT" description="Sent in response to a SendActivityProperties command">
        <Qualifier value="2" name="TelemetryLevel"/>
        <VariableRef name="ActivityId"/>
        <VariableRef name="ActivityName"/>
        <VariableRef name="ActivityPriority"/>
        <VariableRef name="StartTime"/>
        <!-- Additional VariableRefs that apply to all activities for a specific planner are defined here -->
        <!-- Additional VariableRefs for activity unique parameters are defined here -->
      </DataMsg>
    </Notification>

    <Notification>
      <!-- If VariableRefs are added to this message, each extended message must be given a unique name - e.g. ImagingActivityPropertiesUpdate -->
      <DataMsg name="ActivityPropertiesUpdate" id="10" msgArrival="EVENT" description="Activity properties sent each time any of the properties change">
        <Qualifier value="2" name="TelemetryLevel"/>
        <VariableRef name="ActivityId"/>
        <VariableRef name="ActivityName"/>
        <VariableRef name="ActivityPriority"/>
        <VariableRef name="StartTime"/>
        <!-- Additional VariableRefs that apply to all activities for a specific planner are defined here -->
        <!-- Additional VariableRefs for activity unique parameters are defined here -->
      </DataMsg>
    </Notification>

  </Interface>


